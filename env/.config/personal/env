#!/usr/bin/env zsh

# Super cool keyboard stuffs.
# Required for x applications
setxkbmap -option caps:ctrl_modifier
# Use Dvorak Customize Keyboard Layout
setxkbmap pp

# opam configuration
[[ ! -r /home/ppp/.opam/opam-init/init.zsh ]] || source /home/ppp/.opam/opam-init/init.zsh  > /dev/null 2> /dev/null
